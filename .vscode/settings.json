{
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "html",
        "markdown"
    ],
    "[plaintext]": {
        "files.insertFinalNewline": false,
        "files.trimFinalNewlines": false
    },
}
